{
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"color": {
			"palette": [
				{
					"name": "megane-Primary",
					"slug": "megane-primary",
					"color": "#0073aa"
				}
			]
		},
		"custom": {
			"line-height": {
				"megane-body": 1.7,
				"megane-heading": 1.3
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Noto Sans CJK JP\",sans-serif",
					"slug": "megane-system-font",
					"name": "Megane System Font"
				}
			],
			"fontSizes": [
				{
					"slug": "megane-min",
					"size": "14px",
					"name": "Megane Min"
				},
				{
					"slug": "megane-normal",
					"size": "16px",
					"name": "Megane Normal"
				},
				{
					"slug": "meagne-big",
					"size": "32px",
					"name": "Megane Big"
				},
				{
					"slug": "meagne-super-big",
					"size": "50px",
					"name": "Megane Super Big"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--megane-system-font)",
			"fontSize": "var(--wp--preset--font-size--megane-normal)",
			"lineHeight": "var(--wp--custom--line-height--megane-body)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--megane-primary)"
				}
			}
		}
	}
}